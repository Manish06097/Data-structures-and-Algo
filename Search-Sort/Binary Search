#include <iostream>
#include <vector>
using namespace std;


int binarySearch(vector<int> v, int find)

{
    int low=0,high=v.size()-1;
    int mid;
    while(low<=high)
    {
        int mid=low+(high-low) /2;
    
        if(v[mid] == find)
            return mid;
        if(v[mid]<find)
        {
            low=mid+1;
        }
        else
        {
            high = mid - 1;
        }
        
    }
    return -1;
}

int main(void) {
  vector<int> arr = {3, 4, 5, 6, 7, 8, 9};
  int x=8;
  int result = binarySearch(arr, x);
  if (result == -1)
    printf("Not found");
  else
    printf("Element is found at index %d", result);
}
